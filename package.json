{
  "name": "dnd-encounter-tracker",
  "version": "0.1.0",
  "private": true,
  "type": "module",
  "homepage": "https://heathervv.github.io/dnd-encounter-tracker/",
  "scripts": {
    "start": "vite",
    "build": "vite build",
    "serve": "vite preview",
    "typecheck": "tsc -p tsconfig.app.json --pretty false",
